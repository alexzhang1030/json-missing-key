{
  "name": "json-missing-key",
  "type": "module",
  "version": "0.2.0",
  "description": "Compares two json files, shows missing keys.",
  "author": "alexzhang1030",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexzhang1030/json-missing-key.git"
  },
  "keywords": [
    "cli",
    "diff",
    "json",
    "missing key"
  ],
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "play": "tsx src/index.ts",
    "prepare": "simple-git-hooks",
    "build": "tsup",
    "prepublishOnly": "tsup",
    "release": "bumpp --commit \"chore: release v%s\" --push --tag && pnpm publish",
    "lint": "eslint ."
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "codechalk": "1.0.1-beta.2",
    "consola": "^3.4.2",
    "is-object": "^1.0.2",
    "meow": "^13.2.0",
    "object-path": "^0.11.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@types/is-object": "^1.0.5",
    "@types/node": "^22.13.14",
    "@types/object-path": "^0.11.4",
    "@types/traverse": "^0.6.37",
    "bumpp": "^10.1.0",
    "eslint": "^9.23.0",
    "lint-staged": "^15.5.0",
    "simple-git-hooks": "^2.12.1",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,json,md,yaml,yml}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": "@antfu",
    "rules": {
      "no-console": "off"
    }
  }
}
