{
  "name": "json-missing-key",
  "type": "module",
  "version": "0.1.5",
  "description": "Compares two json files, shows missing keys.",
  "author": "alexzhang1030",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexzhang1030/json-missing-key.git"
  },
  "keywords": [
    "cli",
    "diff",
    "json",
    "missing key"
  ],
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "play": "tsx src/index.ts",
    "prepare": "simple-git-hooks",
    "build": "tsup",
    "prepublishOnly": "tsup",
    "release": "bumpp --commit \"chore: release v%s\" --push --tag && pnpm publish"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "codechalk": "1.0.1-beta.2",
    "consola": "^2.15.3",
    "is-object": "^1.0.2",
    "meow": "^11.0.0",
    "object-path": "^0.11.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@types/is-object": "^1.0.2",
    "@types/node": "^18.14.0",
    "@types/object-path": "^0.11.1",
    "@types/traverse": "^0.6.32",
    "bumpp": "^8.2.1",
    "eslint": "^8.34.0",
    "lint-staged": "^13.1.2",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.6.3",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,json,md,yaml,yml}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": "@antfu",
    "rules": {
      "no-console": "off"
    }
  }
}
